{
  "name": "deep-combination",
  "version": "0.0.1",
  "description": "Multi-dimention pair generator",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/generated.d.ts",
  "scripts": {
    "prepare": "rm -rf dist && npm-run-all build:* generate-dts",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "generate-dts": "ts-node ./scripts/generate-dts.ts",
    "lint": "eslint --cache './{src,scripts}/**/*.ts'",
    "test": "jest"
  },
  "keywords": [
    "combination",
    "deep",
    "pair",
    "dimention",
    "matrix"
  ],
  "author": "Leko <leko.noor@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^13.1.6",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.0-beta"
  }
}
